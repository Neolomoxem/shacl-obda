@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix sh:  <http://www.w3.org/ns/shacl#>.
@prefix ex: <http://example.com/ns#>. 
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>. 
@prefix urn: <http://fliqz.com/> .
@prefix : <urn:absolute/prototyp#> . 


:paramEqual
	a sh:ConstraintComponent ;
	sh:parameter [
		sh:path :paramEqual1 ;
	] ;
	sh:parameter [
		sh:path :paramEqual2 ;
	] ;
	sh:labelTemplate "Values are literals with language \"{$lang}\"" ;
	sh:propertyValidator [
		a sh:SPARQLSelectValidator ;
		sh:message "Values are literals with language \"{?lang}\"" ;
		sh:select """
			SELECT DISTINCT $this ?value
			WHERE {
				$this $PATH ?value .
				FILTER (!isLiteral(?value) || !langMatches(lang(?value), $lang))
			}
			"""
	] .



:ParameterName
	a sh:NodeShape ;
	sh:targetClass 	:MSM-Material ;
	:paramEqual1 :Gitterkonstante_a;
	:paramEqual2 :Gitterkonstante_b .