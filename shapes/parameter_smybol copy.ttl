@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix sh:  <http://www.w3.org/ns/shacl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>. 

@prefix : <urn:absolute/prototyp#> . 

# Ein Parameter soll genau ein Symbol haben

:testShape
	a sh:NodeShape ;
	sh:targetClass :Parameter ;
	sh:property [    # _:b1
		sh:path ex:myProperty ;
		sh:minCount 1 ;
		sh:datatype xsd:string ;
		sh:severity sh:Violation ;
		sh:message "Kein Symbol!"@de ;
	] ;
	sh:property [    # _:b2
		# The default severity here is sh:Violation
		sh:path ex:myProperty ;
		sh:maxCount 1 ;
		sh:message "Uneindeutige (mehrere) Symbole"@de ;
	] .