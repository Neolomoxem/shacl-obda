# Ist jedes Material mindestens ein Symbol?

# Eine Blockierkraft darf nicht einem Elastomermaterial zugeordnet sein.

:A 
	a sh:NodeShape;
	sh:targetClass :MSM-Material;
	sh:property [
		sh:path :hat_Parameter;
		sh:class :Gitterkonstante;
		sh:minCount 3;
		# sh:node :B;
		sh:or (
			[
				sh:path :hat_Wert;
				sh:minCount 2;
				sh:datatype xsd:string;
			]
			[
				sh:class :MSM-Material;
				sh:property [
					sh:path :Abdsc;
					sh:class :Dljkbnfk
				]
			]
		);
		sh:not [
			sh:path :hat_Name;
			sh:hasValue "Gitterkonstante_a";
		]
	] .


:B
	a sh:NodeShape;
	sh:property [
		sh:path :ist_Parameter_von;
		sh:minCount 1;
		sh:maxCount 1
	] .
